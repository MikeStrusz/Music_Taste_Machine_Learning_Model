import pandas as pd

# Load data
df = pd.read_csv("data/liked.csv")

# Apply filters
filtered_df = df[
    (df["Danceability"] >= 0.88) &
    (df["Danceability"] <= 0.92) &
    (df["Key"].isin([0, 1, 2])) &
    (df["Mode"] == 1) &
    (df["Duration (ms)"] >= 120000) &   # 2 minutes
    (df["Duration (ms)"] <= 300000)      # 5 minutes
]

# Select relevant columns (using exact spellings)
final_view = filtered_df[[
    "Track Name",
    "Artist Name(s)",
    "Album Name",
    "Release Date",
    "Duration (ms)",
    "Danceability",
    "Key",
    "Mode",
    "Genres"
]]

# Display all qualifying tracks
final_view



