import pandas as pd

# Load the dataset
df = pd.read_csv('data/pareto.csv')

# Check column names
print("Available columns:", df.columns.tolist())

# Convert ms to minutes
df['Duration (min)'] = df['Duration (ms)'] / 60000

# Apply filters
filtered = df[
    (df['Duration (min)'] >= 2) &
    (df['Duration (min)'] <= 5) &
    (df['Popularity'] >= 78) &
    (df['Popularity'] <= 82) &
    (df['Acousticness'] >= 0.17) &
    (df['Acousticness'] <= 0.23)
]

# Identify songs that did NOT meet criteria
excluded = df.loc[~df.index.isin(filtered.index)]

# Display matching tracks
print("\nTracks that met the criteria:")
print(filtered[[
    'Track Name', 'Artist Name(s)', 'Album Name', 'Release Date',
    'Popularity', 'Acousticness', 'Duration (min)'
]])

# Generate paragraph naming excluded songs
if not excluded.empty:
    excluded_titles = excluded['Track Name'].tolist()
    paragraph = (
        "The following songs from the original 11-track playlist did not meet "
        "the duration, popularity, or acousticness criteria: "
        + ", ".join(excluded_titles)
        + "."
    )
else:
    paragraph = (
        "All songs in the original 11-track playlist met the specified criteria."
    )

print("\nSummary paragraph:")
print(paragraph)



